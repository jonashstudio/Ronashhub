-- üìÇ Sidebar
local sidebar = Instance.new("Frame")
sidebar.Size = UDim2.new(0, 150, 1, -30)
sidebar.Position = UDim2.new(0, 0, 0, 30)
sidebar.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
sidebar.BorderSizePixel = 0
sidebar.Parent = mainFrame

local sidebarCorner = Instance.new("UICorner")
sidebarCorner.CornerRadius = UDim.new(0, 10)
sidebarCorner.Parent = sidebar

-- üì¶ Content Frame
local contentFrame = Instance.new("Frame")
contentFrame.Size = UDim2.new(1, -150, 1, -30)
contentFrame.Position = UDim2.new(0, 150, 0, 30)
contentFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
contentFrame.BorderSizePixel = 0
contentFrame.Parent = mainFrame

local contentCorner = Instance.new("UICorner")
contentCorner.CornerRadius = UDim.new(0, 10)
contentCorner.Parent = contentFrame

-- üìå Pages
local pages = {}
local function createPage(name)
    local page = Instance.new("Frame")
    page.Size = UDim2.new(1, 0, 1, 0)
    page.BackgroundTransparency = 1
    page.Visible = false
    page.Parent = contentFrame
    pages[name] = page
    return page
end

local function showPage(name)
    for _, page in pairs(pages) do
        page.Visible = false
    end
    if pages[name] then
        pages[name].Visible = true
    end
end

-- üñ≤ Sidebar button creator
local function createSidebarButton(name, order, callback)
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(1, -10, 0, 35)
    button.Position = UDim2.new(0, 5, 0, 10 + (order * 40))
    button.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    button.Text = name
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.Font = Enum.Font.GothamBold
    button.TextSize = 14
    button.Parent = sidebar

    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 6)
    btnCorner.Parent = button

    button.MouseEnter:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    end)
    button.MouseLeave:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    end)

    button.MouseButton1Click:Connect(callback)
end

-- üéØ Pages
local statusPage = createPage("Status")
local antiAFKPage = createPage("Anti-AFK")

-- üìå Sidebar buttons
createSidebarButton("Status", 0, function() showPage("Status") end)
createSidebarButton("Anti-AFK", 1, function() showPage("Anti-AFK") end)

-- ‚ö° Status Controls (Speed, Jump, Gravity, Fly)
local player = game.Players.LocalPlayer
local humanoid = player.Character:WaitForChild("Humanoid")
local hrp = player.Character:WaitForChild("HumanoidRootPart")

-- Helper to make label + +/- buttons
local function makeStatControl(parent, text, posY, default, applyFunc)
    local value = default

    local label = Instance.new("TextLabel")
    label.Text = text .. ": " .. value
    label.Size = UDim2.new(0, 250, 0, 30)
    label.Position = UDim2.new(0, 20, 0, posY)
    label.TextColor3 = Color3.new(1,1,1)
    label.BackgroundTransparency = 1
    label.Font = Enum.Font.GothamBold
    label.TextSize = 14
    label.Parent = parent

    local minusBtn = Instance.new("TextButton")
    minusBtn.Text = "-"
    minusBtn.Size = UDim2.new(0, 30, 0, 30)
    minusBtn.Position = UDim2.new(0, 280, 0, posY)
    minusBtn.BackgroundColor3 = Color3.fromRGB(50,50,50)
    minusBtn.TextColor3 = Color3.new(1,1,1)
    minusBtn.Font = Enum.Font.GothamBold
    minusBtn.TextSize = 14
    minusBtn.Parent = parent

    local plusBtn = Instance.new("TextButton")
    plusBtn.Text = "+"
    plusBtn.Size = UDim2.new(0, 30, 0, 30)
    plusBtn.Position = UDim2.new(0, 320, 0, posY)
    plusBtn.BackgroundColor3 = Color3.fromRGB(50,50,50)
    plusBtn.TextColor3 = Color3.new(1,1,1)
    plusBtn.Font = Enum.Font.GothamBold
    plusBtn.TextSize = 14
    plusBtn.Parent = parent

    minusBtn.MouseButton1Click:Connect(function()
        value = math.max(0, value - 5)
        label.Text = text .. ": " .. value
        applyFunc(value)
    end)

    plusBtn.MouseButton1Click:Connect(function()
        value = value + 5
        label.Text = text .. ": " .. value
        applyFunc(value)
    end)
end

-- Controls
makeStatControl(statusPage, "Speed", 20, 16, function(v) humanoid.WalkSpeed = v end)
makeStatControl(statusPage, "Jump", 60, 50, function(v) humanoid.JumpPower = v end)
makeStatControl(statusPage, "Gravity", 100, 196, function(v) workspace.Gravity = v end)

-- ‚úà Fly system
local flying = false
local flySpeed = 50
local flyLabel

makeStatControl(statusPage, "Fly Speed", 140, flySpeed, function(v) flySpeed = v end)

local flyBtn = Instance.new("TextButton")
flyBtn.Text = "Toggle Fly (OFF)"
flyBtn.Size = UDim2.new(0, 200, 0, 40)
flyBtn.Position = UDim2.new(0, 20, 0, 180)
flyBtn.BackgroundColor3 = Color3.fromRGB(50,50,50)
flyBtn.TextColor3 = Color3.new(1,1,1)
flyBtn.Font = Enum.Font.GothamBold
flyBtn.TextSize = 14
flyBtn.Parent = statusPage

local bodyVel

flyBtn.MouseButton1Click:Connect(function()
    flying = not flying
    flyBtn.Text = "Toggle Fly (" .. (flying and "ON" or "OFF") .. ")"

    if flying then
        bodyVel = Instance.new("BodyVelocity")
        bodyVel.MaxForce = Vector3.new(4000,4000,4000)
        bodyVel.Velocity = Vector3.zero
        bodyVel.Parent = hrp

        game:GetService("RunService").RenderStepped:Connect(function()
            if flying and hrp then
                local moveDir = Vector3.zero
                if game.UserInputService:IsKeyDown(Enum.KeyCode.W) then
                    moveDir = moveDir + workspace.CurrentCamera.CFrame.LookVector
                end
                if game.UserInputService:IsKeyDown(Enum.KeyCode.S) then
                    moveDir = moveDir - workspace.CurrentCamera.CFrame.LookVector
                end
                if game.UserInputService:IsKeyDown(Enum.KeyCode.A) then
                    moveDir = moveDir - workspace.CurrentCamera.CFrame.RightVector
                end
                if game.UserInputService:IsKeyDown(Enum.KeyCode.D) then
                    moveDir = moveDir + workspace.CurrentCamera.CFrame.RightVector
                end
                if game.UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                    moveDir = moveDir + Vector3.new(0,1,0)
                end
                if game.UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                    moveDir = moveDir - Vector3.new(0,1,0)
                end
                bodyVel.Velocity = moveDir * flySpeed
            elseif bodyVel then
                bodyVel.Velocity = Vector3.zero
            end
        end)
    else
        if bodyVel then bodyVel:Destroy() end
    end
end)

-- üí§ Anti-AFK Page
local antiAFKEnabled = false
local afkLabel = Instance.new("TextLabel")
afkLabel.Text = "Anti-AFK: OFF"
afkLabel.Size = UDim2.new(0, 250, 0, 30)
afkLabel.Position = UDim2.new(0, 20, 0, 20)
afkLabel.TextColor3 = Color3.new(1,1,1)
afkLabel.BackgroundTransparency = 1
afkLabel.Font = Enum.Font.GothamBold
afkLabel.TextSize = 14
afkLabel.Parent = antiAFKPage

local afkBtn = Instance.new("TextButton")
afkBtn.Text = "Toggle Anti-AFK"
afkBtn.Size = UDim2.new(0, 200, 0, 40)
afkBtn.Position = UDim2.new(0, 20, 0, 60)
afkBtn.BackgroundColor3 = Color3.fromRGB(50,50,50)
afkBtn.TextColor3 = Color3.new(1,1,1)
afkBtn.Font = Enum.Font.GothamBold
afkBtn.TextSize = 14
afkBtn.Parent = antiAFKPage

local conn
afkBtn.MouseButton1Click:Connect(function()
    antiAFKEnabled = not antiAFKEnabled
    afkLabel.Text = "Anti-AFK: " .. (antiAFKEnabled and "ON" or "OFF")

    if antiAFKEnabled then
        conn = game.Players.LocalPlayer.Idled:Connect(function()
            game.VirtualUser:CaptureController()
            game.VirtualUser:ClickButton2(Vector2.new())
        end)
    else
        if conn then conn:Disconnect() end
    end
end)

-- üîò Floating Open Button
local openBtn = Instance.new("TextButton")
openBtn.Size = UDim2.new(0, 60, 0, 60)
openBtn.Position = UDim2.new(0.05, 0, 0.8, 0)
openBtn.Text = "Ronash"
openBtn.BackgroundColor3 = Color3.fromRGB(200, 60, 60)
openBtn.TextColor3 = Color3.new(1,1,1)
openBtn.Font = Enum.Font.GothamBold
openBtn.TextSize = 12
openBtn.Parent = screenGui
openBtn.Visible = false
openBtn.Active = true
openBtn.Draggable = true

local openCorner = Instance.new("UICorner")
openCorner.CornerRadius = UDim.new(1, 0) -- circle
openCorner.Parent = openBtn

closeBtn.MouseButton1Click:Connect(function()
    mainFrame.Visible = false
    openBtn.Visible = true
end)

minBtn.MouseButton1Click:Connect(function()
    mainFrame.Visible = false
    openBtn.Visible = true
end)

openBtn.MouseButton1Click:Connect(function()
    mainFrame.Visible = true
    openBtn.Visible = false
end)

-- Default page
showPage("Status")
